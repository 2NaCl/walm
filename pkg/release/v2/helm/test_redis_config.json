{
    "App": {
      "redis": {
        "image": "transwarp/redis:live",
        "replicas": 1,
        "resources": {
          "storage": {
            "data": {
              "size": "1G",
              "storageClass": "opt-local-disk"
            }
          }
        }
      }
    },
    "Transwarp_Config": {
      "Transwarp_Node_SelectorTerms": [
        {
          "key": "master",
          "operator": "Exists"
        }
      ]
    }
}