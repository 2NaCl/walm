{{- if eq (getv "/security/guardian_plugin_enable" "false") "true" -}}
<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>
<configuration>
{{- range gets "/security/guardian_client_config/guardian_site/*" }}
    <property>
        <name>{{base .Key}}</name>
        <value>{{.Value}}</value>
    </property>
{{- end }}
</configuration>
{{- end }}